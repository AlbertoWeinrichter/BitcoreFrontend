{
  "name": "bitcoregaming",
  "version": "1.0.0",
  "description": "Bitcore front-end project",
  "author": "Alberto Weinrichter",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "test": "jest",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo 'skip lint'"
    }
  },
  "dependencies": {
    "@fortawesome/free-brands-svg-icons": "^5.6.3",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/style-resources": "^0.1.1",
    "@nuxtjs/toast": "^3.0.2",
    "auth0-lock": "11.8.1",
    "aws-sdk": "^2.390.0",
    "bootstrap-vue": "^2.0.0-rc.20",
    "cookie": "^0.4.0",
    "core-js": "^2.6.5",
    "data-uri-to-buffer": "^2.0.1",
    "express": "^4.17.0",
    "jquery": "^3.4.1",
    "js-cookie": "^2.2.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.11",
    "moment": "^2.23.0",
    "node-sass": "^4.11.0",
    "nuxt": "^2.8.0",
    "nuxt-facebook-pixel-module": "^1.3.0",
    "nuxt-fontawesome": "^0.4.0",
    "sass-loader": "^7.1.0",
    "vue": "2.6.10",
    "vue-cookie-law": "^1.8.0",
    "vue-cropperjs": "^4.0.0",
    "vue-image-crop-upload": "^2.5.0",
    "vue-infinite-loading": "^2.4.4",
    "vue-line-clamp": "^1.3.2",
    "vue-moment": "^4.0.0",
    "vue-no-ssr": "^1.1.1",
    "vue-notification": "^1.3.16",
    "vue-quill-editor": "^3.0.6",
    "vue-scrollto": "^2.13.0",
    "vue-social-sharing": "^2.4.3",
    "vue-template-compiler": "2.6.10",
    "vuejs-datepicker": "^1.5.4",
    "yarn": "^1.17.3"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-vue": "^5.2.2",
    "husky": "^2.2.0",
    "nodemon": "^1.11.0",
    "prettier": "^1.17.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/resources/assets/js/components/$1"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ]
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "node": "current"
              }
            }
          ]
        ]
      }
    }
  }
}
